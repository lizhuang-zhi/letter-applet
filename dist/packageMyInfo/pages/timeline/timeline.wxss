page{--line-color:#ffe5c1;background:#fff}.weui-cell-box{margin-top:40rpx}.weui-cell-box .cell-start-line{width:50rpx;height:30rpx;border-top:2rpx solid var(--line-color);border-right:2rpx solid var(--line-color)}.weui-cell-list{margin:0 0 8rpx 40rpx}.weui-cell-circle{border-radius:5px;width:20rpx;height:20rpx;border-color:#f3d36c;background:#ffa82e}.weui-cell-line{margin-left:10rpx;border-left:2rpx solid var(--line-color);color:#444}.weui-cell-time{font-size:14px;padding-left:15px;color:#aaa}.weui-cell-event{padding-top:15px;padding-left:15px;letter-spacing:3rpx}.weui-cell-event .source{padding-top:20rpx;color:#bbb}.weui-cell-event .content{color:#666;word-wrap:break-word}