@keyframes pulldown{0%{transform:translateY(0);opacity:0}to{transform:translateY(100rpx);opacity:1}}@keyframes pullup{0%{transform:translateY(100rpx);opacity:1}to{transform:translateY(0);opacity:0}}.yeo-pull-refresh{position:absolute;top:80rpx;left:50%;margin-left:-63rpx;opacity:0}.yeo-start-loading{animation:pulldown .5s forwards ease}.yeo-end-loading{animation:pullup .5s}